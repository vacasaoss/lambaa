{
    "name": "lambaa",
    "version": "3.6.0",
    "description": "A small framework, with very few dependencies to help build API's using AWS API Gateway & Lambda.",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "repository": {
        "type": "git",
        "url": "https://github.com/vacasaoss/lambaa.git"
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsc",
        "prepare": "tsc",
        "prettier": "prettier --write './**/*.{ts,json,md}'",
        "lint": "eslint --config '.eslintrc.json' 'src/**/*.ts'",
        "test": "mocha \"tests/**/*.tests.ts\"",
        "coverage": "nyc mocha \"tests/**/*.tests.ts\""
    },
    "keywords": [
        "api",
        "gateway",
        "lambda",
        "typescript",
        "decorators",
        "routing"
    ],
    "author": "Vacasa",
    "license": "MIT",
    "devDependencies": {
        "@types/aws-lambda": "^8.10.91",
        "@types/chai": "^4.3.0",
        "@types/chai-as-promised": "^7.1.4",
        "@types/mocha": "^9.1.0",
        "@types/node": "^17.0.10",
        "@types/sinon": "^10.0.8",
        "@types/sinon-chai": "^3.2.8",
        "@typescript-eslint/eslint-plugin": "^5.10.0",
        "@typescript-eslint/parser": "^5.10.0",
        "chai": "^4.3.4",
        "chai-as-promised": "^7.1.1",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "husky": "^7.0.4",
        "mocha": "^9.1.4",
        "nyc": "^15.1.0",
        "prettier": "^2.5.1",
        "pretty-quick": "^3.1.3",
        "sinon": "^12.0.1",
        "sinon-chai": "^3.6.0",
        "ts-node": "^10.4.0",
        "typescript": "^4.5.5"
    },
    "dependencies": {
        "reflect-metadata": "^0.1.13"
    },
    "nyc": {
        "extension": [
            ".ts"
        ],
        "check-coverage": true,
        "lines": 90,
        "functions": 90,
        "statements": 90,
        "reporter": [
            "lcov",
            "html"
        ]
    }
}
